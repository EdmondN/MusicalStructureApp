<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/music_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DanceActivity"
            android:label="@string/category_dance"/>
        <activity android:name=".HiphopActivity"
            android:label="@string/category_hiphop"/>
        <activity android:name=".PopActivity"
            android:label="@string/category_pop"/>
        <activity android:name=".LatinActivity"
            android:label="@string/category_latin"></activity>
    </application>

</manifest>